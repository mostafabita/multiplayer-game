{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "indent": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "linebreak-style": "off",
    "arrow-parens": ["error", "always"],
    "no-unused-vars": "off",
    "max-len": "off"
  }
}
